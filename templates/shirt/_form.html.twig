<div class="col-12">
    {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
    <div class="mb-3">
        {{ form_row(form.title) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.description) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.imagesFile) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.upload_date) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.category) }}
    </div>
    <button class="btn btn-outline-success">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}
</div>
