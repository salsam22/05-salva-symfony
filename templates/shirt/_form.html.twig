<div class="col-12">
    {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
    <div class="mb-3">
        {{ form_row(form.title, { 'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.description, { 'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.imagesFile, { 'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.upload_date, { 'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.category, { 'attr': {'class': 'form-control'}}) }}
    </div>
    <button class="btn btn-outline-success">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}
</div>
